<hr />
Card number: <%= item.number %> <br />
Card company: <%= item.company %> <br />
Telephone: <%= item.telephone %> <br />
Mobile: <%= item.mobile %> <br />
QQ: <%= item.mobile %> <br />
Skype: <%= item.skype %> <br />
Link to google drive: <%= item.linkToDrive %> <br />
Date added: <%= item.createdAt %>

<% if(item.details.length > 0) { %>
<table>
  <tr>
    <th>
      Price
    </th>
    <th>
      Art. No
    </th>
    <th>
      Code
    </th>
    <th>
      Cbm
    </th>
    <th>
      Qty/Box
    </th>
    <th>
      Created at
    </th>
    <th>
      Product
    </th>
  </tr>
  <% item.details.forEach(function(detail) { %>
  <tr>
    <td>
      <%= detail.price %>
    </td>
    <td>
      <%= detail.artNo %>
    </td>
    <td>
      <%= detail.code %>
    </td>
    <td>
      <%= detail.cbm %>
    </td>
    <td>
      <%= detail.qtyInBox %>
    </td>
    <td>
      <%= detail.createdAt %>
    </td>
    <td>
      <%= detail.product %>
    </td>
  </tr>
  <% }) %>
</table>
<% } %>
<hr/>
